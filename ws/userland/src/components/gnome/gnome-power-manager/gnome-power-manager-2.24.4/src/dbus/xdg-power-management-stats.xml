<?xml version="1.0" encoding="UTF-8"?>
<node name="/">
  <!-- ************ THIS IS NOT STANDARDISED. DO NOT USE. ************ -->
  <interface name="org.freedesktop.PowerManagement.Statistics">
    <method name="GetDataTypes">
      <arg type="as" name="types" direction="out"/>
    </method>
    <method name="GetParameters">
      <arg type="s" name="type" direction="in"/>
      <arg type="s" name="axis_type_x" direction="out"/>
      <arg type="s" name="axis_type_y" direction="out"/>
      <arg type="s" name="axis_desc_x" direction="out"/>
      <arg type="s" name="axis_desc_y" direction="out"/>
      <arg type="a(is)" name="key_data" direction="out"/><!-- colour, description -->
      <arg type="a(iiis)" name="key_event" direction="out"/><!-- id, colour, shape, description -->
    </method>
    <method name="GetData">
      <arg type="s" name="type" direction="in"/>
      <arg type="a(iii)" name="data" direction="out"/><!-- x, y, colour -->
    </method>
    <method name="GetEventLog">
<!--      <arg type="s" name="type" direction="in"/>-->
      <arg type="a(ii)" name="data" direction="out"/><!-- x, id -->
    </method>
  </interface>
</node>

